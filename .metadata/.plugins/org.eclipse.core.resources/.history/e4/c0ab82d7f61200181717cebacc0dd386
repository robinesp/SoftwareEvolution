module Main

import IO;

set[str] wordsInFile(loc file) 
  = { word | /<word:[a-zA-Z]+>/ := readFile(file)};
  
 
void readRequirements() {
	highFiles = |project://Assignment1/modis/high|.ls;
	lowFiles = |project://Assignment1/modis/low|.ls;
	
	list[list[str]] highReqs;
	list[list[str]] lowReqs;
	
	for (f <- highFiles) {
		highReqs + wordsInFile(f);
	}
	
	for (f <- lowFiles) {
		lowReqs + wordsInFile(f);
	}
	
	print("<lowReqs>");
}

void detect() {
	readRequirements();
}